<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<TilePane fx:id="raiz" prefColumns="2" prefRows="2" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mat.ciclos.matricularciclos.VentanaMostrarController">
   <children>
      <Label fx:id="labelAlumno" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="17.0" prefWidth="200.0" textFill="#2212cc">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font></Label>
      <ImageView fx:id="fotoAlumno" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/pngwing.com.png" />
         </image></ImageView>
      <TableView fx:id="tablaModulosAlumno" prefHeight="200.0" prefWidth="200.0" TilePane.alignment="BOTTOM_LEFT">
        <columns>
          <TableColumn fx:id="nombreModuloCol" maxWidth="10000.0" prefWidth="100.0" text="Modulo" />
          <TableColumn fx:id="horaModuloCol" maxWidth="3000.0" prefWidth="75.0" text="Horas" />
        </columns>
         <TilePane.margin>
            <Insets bottom="20.0" left="20.0" />
         </TilePane.margin>
      </TableView>
      <Button fx:id="cerrar" alignment="BOTTOM_RIGHT" mnemonicParsing="false" onAction="#recuperarDatos" text="Recuperar Datos" textAlignment="CENTER" TilePane.alignment="BOTTOM_RIGHT">
         <TilePane.margin>
            <Insets bottom="20.0" right="50.0" />
         </TilePane.margin></Button>
   </children>
</TilePane>
